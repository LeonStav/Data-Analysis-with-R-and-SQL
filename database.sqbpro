SELECT player_name,
	height, 
	weight 
FROM Player 
WHERE height BETWEEN 160 AND 180 
ORDER BY weight DESC;

--Δεύτερο ερώτημα
SELECT date, team_long_name as Home_team, 
	home_team_goal as Home_team_goals 
FROM Match 
LEFT JOIN Team 
ON Match.home_team_api_id = Team.team_api_id 
WHERE match_api_id = 492473;
	
--Τρίτο ερώτημα
SELECT DISTINCT Team.team_long_name as Team_name, 
	Country.name as Country
FROM Team JOIN Match JOIN Country
ON Match.home_team_api_id = Team.team_api_id AND Country.id = Match.country_id;

--Τέταρτο ερώτημα
SELECT date, 
	(SELECT T.team_long_name FROM Match M JOIN Team T ON M.home_team_api_id = T.team_api_id WHERE match_api_id = 492473) as Home_Team, 
	home_team_goal as Home_team_goals,
	(SELECT T.team_long_name FROM Match M JOIN Team T ON M.away_team_api_id = T.team_api_id WHERE match_api_id = 492473) as Away_team, 
	away_team_goal as Away_team_goals
FROM Match
WHERE match_api_id = 492473;
	
	</sql><current_tab id="0"/></tab_sql></sqlb_project>
